    "@react-pdf/renderer": "^1.6.13",
    "@sendgrid/mail": "^7.4.2",
    "@stripe/react-stripe-js": "^1.2.2",
    "@stripe/stripe-js": "^1.12.1",
    "@types/express": "^4.17.1",
    "@types/mongodb": "^3.3.3",
    "@types/mongoose": "^5.5.18",
    "@types/node": "^12.12.7",
    "airtable": "0.8.1",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
